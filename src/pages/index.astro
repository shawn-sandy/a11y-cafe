---
import Layout from "../layouts/Layout.astro";
import CollectionList from "#components/CollectionList.astro";
---

<Layout
  pageTitle="We make Accessibility fun!"
  showBreadcrumb={false}
  showFeature={true}
>
<section>
  <CollectionList itemsPerPage={5} collection="posts" category="articles" />
</section>
<hr>
  <section>
    <CollectionList showPagination={true} itemsPerPage={5} collection="posts" omitCategory="articles" />
  </section>
</Layout>
